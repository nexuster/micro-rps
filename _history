     # . # . .\n            . # . . .\n            `)\n    }\n    basic.pause(1000)\n    menu()\n}\nfunction startGame () {\n    inGame = true\n    basic.showNumber(3)\n    basic.showNumber(2)\n    basic.showNumber(1)\n    basic.showNumber(0)\n    rps = randint(0, 2)\n    if (rps == 0) {\n        basic.showLeds(`\n            . . . . .\n            # . # # #\n            . # . . .\n            # . # # #\n            . . . . .\n            `)\n    } else {\n        if (rps == 1) {\n            basic.showLeds(`\n                . . . . .\n                . . # # .\n                . # # # #\n                # # # # #\n                . . . . .\n                `)\n        } else {\n            if (rps == 2) {\n                basic.showLeds(`\n                    # # # # .\n                    # # # # #\n                    # # # # #\n                    # # # # #\n                    # # # # #\n                    `)\n            }\n        }\n    }\n    basic.pause(500)\n    radio.sendNumber(rps)\n}\ninput.onButtonPressed(Button.A, function () {\n    radId += -1\n})\ninput.onButtonPressed(Button.AB, function () {\n    inGame = true\n})\ninput.onButtonPressed(Button.B, function () {\n    radId += 1\n})\nlet ableToPlay = false\nlet lost = 0\nlet rps = 0\nlet inGame = false\nlet radId = 0\ninGame = false\nmenu()\n","README.md":"","pxt.json":"{\n    \"name\": \"rock paper scissors\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":1741272119835,"editorVersion":"7.0.57","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"$w4|GS#-l_.![!RhE[Gn\">inGame</variable><variable id=\"NX6?jjoE.Y]Xh}7b4wv)\">rps</variable><variable id=\"vLY%7bYK6]=:_)`lp{j3\">lost</variable><variabl